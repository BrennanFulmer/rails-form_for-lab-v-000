
<h3>Student Form</h3>

<%= form_for(@student) do |f| %>
  <%= label_tag "Student First Name" %>
  <br>
  <%= f.text_field :student_first_name %>
  <br>
  
  <%= label_tag "Student Last Name" %>
  <br>
  <%= f.text_field :student_last_name %>
  <br>

  <%= f.submit %> <% # Create Student %>
<% end %>