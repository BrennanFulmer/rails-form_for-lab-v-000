
<h3>School Class Form</h3>

<%= form_for(@school_class) do |f| %>
  <%= label_tag "School Class Title" %>
  <br>
  <%= f.text_field :school_class_title %>
  <br>
  
  <%= label_tag "School Classroom Number" %>
  <br>
  <%= f.text_field :school_class_room_number %>
  <br>

  <%= f.submit %> <% # Create School class %>
<% end %>